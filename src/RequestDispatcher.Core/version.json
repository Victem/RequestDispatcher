{
  "$schema": "https://raw.githubusercontent.com/dotnet/Nerdbank.GitVersioning/main/src/NerdBank.GitVersioning/version.schema.json",
  "version": "0.0.1",
  "assemblyVersion": {
    "precision": "revision"
  },
  "publicReleaseRefSpec": [
    "^refs/heads/master$",
    "^refs/heads/release/request-dispatcher.core/v\\d*\\.\\d*.\\d*$"
  ],
  "release": {
    "tagName": "request-dispatcher.core/v{version}",
    "branchName": "release/request-dispatcher.core/v{version}",
    "versionIncrement": "build"
  },
  "pathFilters": [
    "./"
  ]
}
